<script>
  $(document).ready(function () {
    var windowWidth = $(window).width();
    if (windowWidth <= 700) {
      $("#myVideo").html('<source src="video/m_Walking.mp4" type="video/mp4"></source>');
      $("#myVideo")[0].load();
    }
    $('.cropping').css('height', $(window).height());
  });
</script>

